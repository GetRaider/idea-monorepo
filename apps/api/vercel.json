{
  "version": 2,
  "installCommand": "pnpm install --recursive --frozen-lockfile",
  "buildCommand": "pnpm build",
  "builds": [
    {
      "src": "src/vercel.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/vercel.ts"
    }
  ],
  "outputDirectory": "dist"
}
