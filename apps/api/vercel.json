{
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "pnpm build",
  "framework": null,
  "public": true,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/main.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "OPTIONS",
        "DELETE",
        "HEAD",
        "CONNECT",
        "TRACE"
      ],
      "headers": { "Access-Control-Allow-Origin": "*" }
    }
  ],
  "outputDirectory": "dist"
}
