{
  "version": 2,
  "builds": [
    {
      "src": "/apps/api/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/packages/api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/apps/api/(.*)",
      "dest": "/apps/api/src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
    }
  ],
  "outputDirectory": "dist"
}
